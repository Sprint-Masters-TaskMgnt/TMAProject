@{
    ViewBag.Title = "Contact";
    ViewBag.Message = "Your contact page.";
}
<main aria-labelledby="title" class="container mt-4">
    <h2 id="title" class="text-primary">@ViewBag.Title</h2>
    <h3 class="text-secondary">@ViewBag.Message</h3>

    <p>If you have any questions, suggestions, or need assistance, feel free to reach out to us!</p>

    <section class="mt-4">
        <h4>Contact Information</h4>
        <address>
            <p>
                <strong>Task Management System </strong><br>
                Address:
                Gopalan Promenade Mall, 3rd floor, Kathreguppe, Banashankari 3rd Stage<br>
                City:
                Bangalore, KA

                Zip Code:
                560085<br>
                P: (123) 456-7890
            </p>
            <p>
                <strong>Support:</strong> <a href="mailto:support@taskmanagement.com">support@taskmanagement.com</a><br>
                <strong>Sales:</strong> <a href="mailto:sales@taskmanagement.com">sales@taskmanagement.com</a><br>
                <strong>Marketing:</strong> <a href="mailto:marketing@taskmanagement.com">marketing@taskmanagement.com</a>
            </p>
        </address>
    </section>

    <section class="mt-4">
        <h4>Stay Connected</h4>
        <p>Follow us on social media to stay updated with the latest features and tips!</p>
        <ul class="list-inline">
            <li class="list-inline-item">
                <a href="https://www.facebook.com" target="_blank" class="btn btn-primary">Facebook</a>
            </li>
            <li class="list-inline-item">
                <a href="https://www.twitter.com" target="_blank" class="btn btn-info">Twitter</a>
            </li>
            <li class="list-inline-item">
                <a href="https://www.linkedin.com" target="_blank" class="btn btn-secondary">LinkedIn</a>
            </li>
        </ul>
    </section>

    <section class="mt-4">
        <h4>Visit Us</h4>
        <p>We'd love to see you! Here's our location:</p>
        <div>
            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3153.0198939196625!2d-122.1419516846831!3d37.44188397982459!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x808fba03b1b1b91f%3A0x4f1f2c529d63f96!2s123%20Productivity%20Ln%2C%20Menlo%20Park%2C%20CA%2094025%2C%20USA!5e0!3m2!1sen!2sin!4v1605201262892!5m2!1sen!2sin"
                    width="100%" height="300" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
        </div>
    </section>
</main>